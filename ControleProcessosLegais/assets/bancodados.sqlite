CREATE TABLE clientes(id INTEGER PRIMARY KEY AUTOINCREMENT,
					nome_completo VARCHAR(30),
					rg VARCHAR(30),
					cpf VARCHAR(30),
					orgao_expedidor VARCHAR(30),
					rua VARCHAR(30),
					numero VARCHAR(30),
					cep VARCHAR(30),
					bairro VARCHAR(30),
					estado VARCHAR(30),
					nacionalidade VARCHAR(30),
					estado_civil VARCHAR(30),
					profissao VARCHAR(30),
					cnpj VARCHAR(30),
					ctps VARCHAR(30),
					pis_pasep VARCHAR(30),
					inscricao_estadual VARCHAR(30),
					pai VARCHAR(30),
					mae VARCHAR(30));